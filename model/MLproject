# define args for train model
name: pf-toy-model

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      alpha: {type: float, default: 0.5}
      l1_ratio: {type: float, default: 0.1}
      data_month: {type: int, default: 5}
    command: "python train.py {alpha} {l1_ratio} {data_month}"